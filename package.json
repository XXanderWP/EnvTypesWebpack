  "homepage": "https://github.com/XXanderWP/EnvTypesWebpack#readme",
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/webpack": "^5.28.5",
